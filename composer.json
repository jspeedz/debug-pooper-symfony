{
  "name": "jspeedz/debug-pooper-symfony",
  "description": "Some more debugging methods to be used in a symfony framework (or standalone)",
  "keywords": [
    "dump",
    "debug"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "jspeedz",
      "role": "Developer"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "Jspeedz\\DebugPooper\\": "src/"
    },
    "files": [
      "src/Component/VarDumper/Resources/functions/dump.php",
      "src/Component/VarDumper/Resources/functions/dumpquery.php"
    ],
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "php": "^7.1",
    "doctrine/dbal": "^2.8",
    "ext-PDO": "^7.1",
    "symfony/var-dumper": "^4.1",
    "phpunit/phpunit": "^7.5@dev"
  }
}
